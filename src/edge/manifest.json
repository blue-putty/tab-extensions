{
  "manifest_version": 3,
  "homepage_url": "https://tab.gladly.io",
  "description": "Raise money for charity with every browser tab you open!",
  "chrome_url_overrides": {
    "newtab": "iframe.html"
  },
  "icons": {
    "16": "./img/logo16.png",
    "32": "./img/logo32.png",
    "48": "./img/logo48.png",
    "128": "./img/logo128.png"
  },
  "name": "Tab for a Cause",
  "background": {
    "service_worker": "ext-background.js"
  },
  "permissions": [],
  "version": "1.0"
}
