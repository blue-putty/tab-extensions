{
  "manifest_version": 2,
  "homepage_url": "https://tab.gladly.io",
  "description": "Raise money for charity with every browser tab you open!",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "./img/logo16.png",
    "32": "./img/logo32.png",
    "48": "./img/logo48.png",
    "128": "./img/logo128.png"
  },
  "browser_action": {
    "browser_style": false,
    "default_icon": {
      "16": "./img/logo16.png",
      "32": "./img/logo32.png"
    }
  },
  "name": "Tab for a Cause",
  "background": {
    "scripts": ["ext-background.js"]
  },
  "permissions": [],
  "version": "5.13"
}
